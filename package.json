{
  "name": "zinfer",
  "version": "0.1.0",
  "description": "Extract input/output types from Zod schemas",
  "keywords": [
    "schema",
    "type-extraction",
    "typescript",
    "zod"
  ],
  "license": "MIT",
  "bin": {
    "zinfer": "./bin/zinfer"
  },
  "files": [
    "bin",
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsgo --noEmit",
    "typecheck:tsc": "tsc --noEmit",
    "lint": "oxlint",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "generate:type-tests": "node ./dist/cli.js 'tests/fixtures/*.ts' --outDir tests/__file_snapshots__ --outPattern '[name].ts' --generate-tests --suffix Schema"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "glob": "^13.0.0",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@typescript/native-preview": "7.0.0-dev.20260108.1",
    "lefthook": "^2.0.13",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0",
    "zod": ">=3.0.0"
  },
  "packageManager": "pnpm@10.27.0"
}
